fn main() {
    print("Testing try-catch control flow:")
    
    // Test 1: No error case
    try {
        let result = 10 / 2
        print("No error: result = {}", result)
        // This should print "No error: result = 5"
    } catch error {
        print("Unexpected error: {}", error)
        // This should NOT execute
    }
    
    // Test 2: Error case  
    try {
        let result = 10 / 0
        print("This should not print: {}", result)
        // This should NOT execute
    } catch error {
        print("Expected error: {}", error)
        // This should execute
    }
    
    print("Done")
}