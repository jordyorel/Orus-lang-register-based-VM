fn calculateSum(n: i32) -> i32 {
    let mut sum: i32 = 0
    print("calculateSum called with n = {}", n)
    print("Range: 1 to {}", n + 1)
    
    for i in 1..(n + 1) {
        print("Loop: i = {}, sum = {}", i, sum)
        sum = sum + i
        if i > 20 {  // Safety break
            print("Breaking due to safety limit")
            break
        }
    }
    return sum
}

fn main() {
    let result = calculateSum(5)
    print("Result: {}", result)
}