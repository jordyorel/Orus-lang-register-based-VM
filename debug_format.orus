fn main() {
    print("Testing format step by step...")
    
    // Test each operation separately
    let a = 5 * 3
    print("5 * 3 = {}", a)
    
    let b = 5 / 2
    print("5 / 2 = {}", b)
    
    let c = 3 % 2
    print("3 % 2 = {}", c)
    
    // Test combining operations
    let result = a + b - c
    print("result = {}", result)
}