fn main() {
    let x: i32 = 5
    let y: i64 = 10
    
    // Test interpolation with stored type_of results
    let x_type = type_of(x)
    let y_type = type_of(y)
    
    print("Stored type interpolation:")
    print("Type of x: {}", x_type)
    print("Type of y: {}", y_type)
    
    print("Direct type_of interpolation:")
    print("Type of x: {}", type_of(x))
}