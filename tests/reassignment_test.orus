// Test variable reassignment
let a = 42
print("Initial value of a:")
print(a)

// Reassign a new value
a = 100
print("After reassignment:")
print(a)

// Test with different types
let b = 3.14
print("Initial value of b:")
print(b)

b = 2.71828
print("After reassignment:")
print(b)

// Test with expressions
let c = 10
print("Initial value of c:")
print(c)

c = c + 5
print("After adding 5:")
print(c)

c = c * 2
print("After multiplying by 2:")
print(c)

// Test with complex expressions
let d = 1
print("Initial value of d:")
print(d)

d = (d + 3) * 4
print("After (d + 3) * 4:")
print(d)

// End of test
print("Test complete")
