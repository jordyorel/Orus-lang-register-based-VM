// Test i64 comparison operations
fn main() {
    let a = 3000000000
    let b = 3000000020
    let c = 3000000000

    print("i64 Comparisons:")

    print("a == b is: {}", a == b)
    print("a == c is: {} ", a == c)
    print("a != b is: {} ", a != b)
    print("a < b is: {} ", a < b)
    print("a <= b is: {} ", a <= b)
    print("a <= c is: {} ", a <= c)
    print("a > b is: {} ", a > b)
    print("a >= b is: {}", a >= b)
    print("a >= c is: {} ", a >= c)
}
