// Test automatic type conversions
fn test_type_conversions() -> i32 {
    let i32_val: i32 = 42
    let u32_val: u32 = 100
    let f64_val: f64 = 3.14
    
    print("Type Conversion Test:")
    
    // i32 to f64 conversion
    let conv1 = i32_val + f64_val
    print("i32 + f64 conversion (42 + 3.14):")
    print(conv1)
    
    // u32 to f64 conversion
    let conv2 = u32_val + f64_val
    print("u32 + f64 conversion (100 + 3.14):")
    print(conv2)
    
    // operation result conversions
    let div_result = i32_val / u32_val
    print("Division operation (42 / 100):")
    print(div_result)
    
    return 0
}

test_type_conversions()
