// Test max function with generic type - previously failed
fn max<T>(a: T, b: T) -> T {
    if a > b {
        return a
    } else {
        return b
    }
}

fn main() {
    let result1 = max<i32>(10, 20)
    print("max<i32>(10, 20) = {}", result1)
    
    let result2 = max<f64>(3.14, 2.71)
    print("max<f64>(3.14, 2.71) = {}", result2)
    
    print("Generic comparison operators working!")
}