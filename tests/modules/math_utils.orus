// Math utilities module for testing
pub const PI = 3.14159265359
pub const E = 2.71828182846

pub fn square(x: f64) -> f64 {
    return x * x
}

pub fn cube(x: f64) -> f64 {
    return x * x * x
}

pub fn max(a: i32, b: i32) -> i32 {
    if a > b {
        return a
    } else {
        return b
    }
}

pub fn min(a: i32, b: i32) -> i32 {
    if a < b {
        return a
    } else {
        return b
    }
}

pub fn abs(x: i32) -> i32 {
    if x < 0 {
        return -x
    } else {
        return x
    }
}

fn private_calculation() -> f64 {
    return PI * E  // This should not be accessible
}

fn main() {
    print("Math utilities module loaded")
    print("Available functions: square, cube, max, min, abs")
    print("Available constants: PI, E")
}