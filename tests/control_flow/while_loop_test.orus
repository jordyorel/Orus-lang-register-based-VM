// Test while loop control flow

fn main() {
    let mut count = 0
    let mut sum = 0

    print("While Loop Test:")

    while count < 5 {
        count = count + 1
        sum = sum + count
        print("Iteration:")
        print(count)
        print("Current sum:")
        print(sum)
    }

    print("Final sum:")
    print(sum)
}

