// Test for loop

print("===== FOR LOOP TEST =====")

// Simple for loop
print("Counting from 0 to 4:")
for i in 0..5 {
    print(i)
}

// For loop with expression range
print("Counting with expressions:")
let start = 1
let end = 6
for j in start..end {
    print(j)
}

// Nested for loops
print("===== NESTED FOR LOOPS =====")
for x in 0..3 {
    print("Outer loop:")
    print(x)
    
    for y in 0..2 {
        print("  Inner loop:")
        print(y)
    }
}

print("===== TEST COMPLETE =====")
