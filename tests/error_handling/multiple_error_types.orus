// Multiple error types test
fn main() {
    print("Testing multiple error types...")
    
    // Test division by zero
    try {
        let x = 1 / 0
    } catch div_error {
        print("Division error: {}", div_error)
    }
    
    // Test array bounds
    try {
        let arr = [1, 2, 3]
        let val = arr[5]
    } catch bounds_error {
        print("Bounds error: {}", bounds_error)
    }
    
    // Test modulo by zero
    try {
        let x = 10 % 0
    } catch mod_error {
        print("Modulo error: {}", mod_error)
    }
    
    print("All error types tested")
}