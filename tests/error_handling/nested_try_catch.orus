// Nested try-catch blocks test
fn main() {
    print("Testing nested try-catch...")
    
    try {
        print("Outer try block")
        
        try {
            print("Inner try block")
            let a = 5
            let b = 0
            let c = a / b  // Inner error
            print("This should not print")
        } catch inner_error {
            print("Inner catch: {}", inner_error)
            
            // Throw another error in catch block
            let x = 1
            let y = 0
            let z = x / y  // Another error
        }
        
        print("This should not print either")
    } catch outer_error {
        print("Outer catch: {}", outer_error)
    }
    
    print("Program continues after nested error handling")
}