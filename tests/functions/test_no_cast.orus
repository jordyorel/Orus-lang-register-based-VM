fn add(a: i32, b: i32) -> i32 {
    return a + b
}

fn main() {
    print("Testing without explicit casts:")
    
    // This should work without casts now
    let result = add(5, 7)
    print(result)
    
    // Test with variables too
    let x = 10
    let y = 20
    let sum = add(x, y)
    print(sum)
}