// Test mixed type operations
// This file uses only newlines as statement terminators

// Integer to float coercion
print("===== Integer to Float Coercion =====")

// Integer first, float second
let a = 42
let b = 3.14
let c = a + b  // Should convert a to float

print("a (integer) =")
print(a)
print("b (float) =")
print(b)
print("a + b =")
print(c)

// Float first, integer second
let d = 2.5
let e = 10
let f = d + e  // Should convert e to float

print("d (float) =")
print(d)
print("e (integer) =")
print(e)
print("d + e =")
print(f)

// Direct mixed type operations
print("===== Direct Mixed Type Operations =====")

// Integer first, float second
let g = 7 + 1.5
print("7 + 1.5 =")
print(g)

// Float first, integer second
let h = 3.5 + 2
print("3.5 + 2 =")
print(h)

// More complex mixed type operations
print("===== Complex Mixed Type Operations =====")

let i = (5 + 2.5) * 3
print("(5 + 2.5) * 3 =")
print(i)

let j = 4.2 * (6 + 3)
print("4.2 * (6 + 3) =")
print(j)

// End of test
print("===== Test Complete =====")
