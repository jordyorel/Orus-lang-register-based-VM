// Test loop boundaries and break/continue behaviour
fn main() {
    // For loop with zero iterations
    for i in 0..0 {
        print(i)
    }

    // While loop that never executes
    let mut count: i32 = 0
    while count > 0 {
        print(count)
        count = count - 1
    }

    // Break and continue at edges
    for j in 0..5 {
        if j == 0 {
            continue
        }
        if j == 3 {
            break
        }
        print(j)
    }
}
