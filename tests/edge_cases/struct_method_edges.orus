// Methods calling other methods
struct Counter { value: i32 }

impl Counter {
    fn new(v: i32) -> Counter {
        return Counter{value: v}
    }

    fn inc(self) {
        self.value = self.value + 1
    }

    fn get(self) -> i32 {
        return self.value
    }

    fn inc_and_get(self) -> i32 {
        self.inc()
        return self.get()
    }
}

fn main() {
    let c: Counter = Counter.new(0)
    print(c.inc_and_get())
}
