// Test array indexing at boundary values and dynamic array expansions
fn main() {
    let arr: [i32; 3] = [10, 20, 30]
    // Access first and last valid indices
    print(arr[0])
    print(arr[2])

    // Dynamic array operations
    let dyn: [i32; 1] = [5]
    push(dyn, 6)
    print(dyn[1])
    pop(dyn)
    print(len(dyn))
}
