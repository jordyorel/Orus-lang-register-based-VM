// Test struct field assignment and modification
struct Counter {
    value: i32,
}

fn main() {
    let mut counter = Counter { value: 0 }
    print("Initial value: {}", counter.value)
    
    counter.value = 42
    print("After assignment: {}", counter.value)
    
    counter.value = counter.value + 10
    print("After increment: {}", counter.value)
}