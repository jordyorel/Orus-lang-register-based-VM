// Test nested structs
struct Address {
    street: string,
    number: i32,
}

struct Company {
    name: string,
    address: Address,
}

fn main() {
    let addr = Address {
        street: "Main Street",
        number: 123,
    }
    
    let company = Company {
        name: "Tech Corp",
        address: addr,
    }
    
    print("Company: {}", company.name)
    print("Street: {}", company.address.street)
    print("Number: {}", company.address.number)
}