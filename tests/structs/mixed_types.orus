// Test struct with mixed field types
struct Person {
    name: string,
    age: i32,
    height: f64,
    active: bool,
}

fn main() {
    let person = Person {
        name: "Alice",
        age: 30,
        height: 5.7,
        active: true,
    }
    
    print("Name: {}", person.name)
    print("Age: {}", person.age)
    print("Height: {}", person.height)
    print("Active: {}", person.active)
}