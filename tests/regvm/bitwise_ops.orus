fn main() {
    let a: i64 = 6
    let b: i64 = 3
    print("6 & 3 = {}", a & b)
    print("6 | 3 = {}", a | b)
    print("6 ^ 3 = {}", a ^ b)
    print("!6 = {}", !a)
    print("6 << 1 = {}", a << (1 as i64))
    print("6 >> 1 = {}", a >> (1 as i64))
    let u: u32 = 8u
    print("8u >> 1 = {}", u >> 1u)
}
