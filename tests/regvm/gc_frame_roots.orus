fn make_array(n: i32) -> [i32] {
    let arr = []
    let mut i: i32 = 0
    while i < n {
        arr.push(i)
        i = i + (1 as i32)
    }
    return arr
}

fn nested_alloc(n: i32) -> i32 {
    let a = make_array(n)
    let b = make_array(n)
    return len(a) + len(b)
}

fn main() {
    print(nested_alloc(50000 as i32))
}
