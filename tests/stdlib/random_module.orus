use std::random

fn main() {
    print(random.randint(1 as i32, 10 as i32))
    print(random.randint(1 as i32, 10 as i32))
    print(random.randint(1 as i32, 10 as i32))
    let colors = ["red", "green", "blue"]
    print(random.choice(colors))
