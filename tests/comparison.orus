// Test comparison operations

// Equal to
print(5 == 5)
print(5 == 6)

// Not equal to
print(5 != 6)
print(5 != 5)

// Less than
print(5 < 10)
print(10 < 5)

// Greater than
print(10 > 5)
print(5 > 10)

// Less than or equal to
print(5 <= 5)
print(5 <= 10)
print(10 <= 5)

// Greater than or equal to
print(5 >= 5)
print(10 >= 5)
print(5 >= 10)

// Variables in comparisons
let a = 5
let b = 10
print(a == b)
print(a != b)
print(a < b)
print(a > b)
print(a <= b)
print(a >= b)
