// Test while loops

// Simple while loop
let i = 0
while i < 5 {
    print(i)
    i = i + 1
}

// While loop with break
let j = 0
while j < 10 {
    if j == 5 {
        break
    }
    print(j)
    j = j + 1
}

// While loop with continue
let k = 0
while k < 5 {
    k = k + 1
    if k == 3 {
        continue
    }
    print(k)
}
