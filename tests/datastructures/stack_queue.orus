// Basic stack and queue operations using arrays
// Advanced impl block features are commented out for Phase 3

fn main() {
    // Stack (LIFO) - using basic array operations
    let stack = []
    stack.push(1)
    stack.push(2) 
    stack.push(3)
    
    print("Stack operations:")
    print("Length after pushes:")
    print(len(stack))
    
    let s1 = stack.pop()
    print("Popped from stack:")
    print(s1)
    print("Length after pop:")
    print(len(stack))

    // Queue (FIFO) - using basic array operations  
    let mut queue = []
    queue.push(10)
    queue.push(20)
    queue.push(30)
    
    print("Queue operations:")
    print("Length after pushes:")
    print(len(queue))
    
    let q1 = queue[0]
    print("First element (dequeue):")
    print(q1)
    
    print("Stack and Queue basic operations completed")
}