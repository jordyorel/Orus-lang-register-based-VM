// Test: Vector of unsigned integers in Orus

fn main() {
    let vu = []
    vu.push(1u)
    vu.push(2u)
    vu.push(3u)
    print("UInt vector: " + vu)
    print("Length: " + len(vu))
    print("First: " + vu[0 as i32] + ", Last: " + vu[len(vu) - (1 as i32)])
    let popped = vu.pop()
    print("Popped: " + popped)
    print("After pop: " + vu)
    print("Iterate:")
    for i in (0 as i32)..len(vu) {
        print("vu[" + i + "]: " + vu[i])
    }
}
