fn main() {
    // Test different types
    let int_val = 42
    let float_val = 3.14
    let bool_val = true
    let str_val = "world"
    
    print("Integer: {}", int_val)
    print("Float: {}", float_val) 
    print("Boolean: {}", bool_val)
    print("String: {}", str_val)
    
    // Test multiple placeholders
    print("Values: {}, {}, {}, {}", int_val, float_val, bool_val, str_val)
    
    // Test expressions in arguments
    print("Math: {} + {} = {}", 5, 3, 5 + 3)
    
    // Test with our new type system
    let i32_val = 100
    let i64_val = 999999999999
    print("i32: {}, i64: {}", i32_val, i64_val)
}